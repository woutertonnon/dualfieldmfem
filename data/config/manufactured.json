{
    "mesh": "./extern/mfem/data/periodic-cube.mesh",
    "outputfile": "example",
    "solver": "GMRES",
    "dt": 0.02,
    "T": 1.0,
    "refinements": 2,
    "order": 1,
    "visualisation": 1,
    "printlevel": 0,
    "viscosity": 0.0,
     "tol": 1e-5,
    "boundary_data_u": "out[0] = 0.;out[1]=0.;out[2]=0.;",
"force_data": "out[0] = -2*M_PI*(2-t)*(t+1)*sin(2*M_PI*x[1])*sin(2*M_PI*x[2])-cos(2*M_PI*x[1]);out[1] = 2*M_PI*(1-t)*(t+1)*sin(2*M_PI*x[0])*cos(2*M_PI*x[2])+sin(2*M_PI*x[2]);out[2] = 2*M_PI*(1-t)*(2-t)*cos(2*M_PI*x[0])*cos(2*M_PI*x[1])-sin(2*M_PI*x[0]);",
"initial_data_u": "out[0] = 2*cos(2*M_PI*x[1]);out[1] = sin(2*M_PI*x[2]);out[2] = sin(2*M_PI*x[0]);",
"initial_data_w": "out[0] = -2*M_PI*cos(2*M_PI*x[2]);out[1] = -2*M_PI*cos(2*M_PI*x[0]);out[2] = 4*M_PI*sin(2*M_PI*x[1]);",
"exact_data_u": "out[0] = (2-t)*cos(2*M_PI*x[1]);out[1] = (t+1)*sin(2*M_PI*x[2]);out[2] = (1-t)*sin(2*M_PI*x[0]);",
"exact_data_w": "out[0] = -2*M_PI*(t+1)*cos(2*M_PI*x[2]);out[1] = -2*M_PI*(1-t)*cos(2*M_PI*x[0]);out[2] = 2*M_PI*(2-t)*sin(2*M_PI*x[1]);"


}
